{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "dev": {
        "dependsOn": ["^dev"],
        "cache": false,
        "persistent": true
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "dist/**"]
      },
      "lint": {
        "cache": true
      },
      "test": {
        "cache": true
      }
    }
  }
  